<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>
    <typeAliases>
        <!-- member -->
        <typeAlias alias="MemberDTO" type="com.teamProject.syusyu.domain.member.MemberDTO"/>
        <typeAlias alias="DlvAddrDTO" type="com.teamProject.syusyu.domain.member.DlvAddrDTO"/>

        <!-- product -->
        <typeAlias alias="CategoryDTO" type="com.teamProject.syusyu.domain.product.CategoryDTO"/>
        <typeAlias alias="ProductDTO" type="com.teamProject.syusyu.domain.product.ProductDTO"/>

        <!-- order -->
        <typeAlias alias="CartProdDTO" type="com.teamProject.syusyu.domain.order.CartProdDTO"/>
        <typeAlias alias="CartTotalDTO" type="com.teamProject.syusyu.domain.order.CartTotalDTO"/>
        <typeAlias alias="OrdDTO" type="com.teamProject.syusyu.domain.order.OrdDTO"/>
        <typeAlias alias="OrdDtlDTO" type="com.teamProject.syusyu.domain.order.OrdDtlDTO"/>

        <!-- cs -->
        <typeAlias alias="NoticeDTO" type="com.teamProject.syusyu.domain.cs.NoticeDTO"/>
        <typeAlias alias="FaqDTO" type="com.teamProject.syusyu.domain.cs.FaqDTO"/>

        <!-- etc -->
        <typeAlias alias="SearchCondition" type="com.teamProject.syusyu.domain.cs.SearchCondition"/>
        <typeAlias alias="CommentDto" type="com.teamProject.syusyu.domain.CommentDto"/>

    </typeAliases>
</configuration>